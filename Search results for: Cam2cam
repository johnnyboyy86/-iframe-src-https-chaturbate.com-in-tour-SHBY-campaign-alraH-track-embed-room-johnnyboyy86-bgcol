  #video_singleton .video-js .vjs-volume-panel .vjs-mute-control.vjs-vol-0 .vjs-control-text::after { background: rgb(0, 0, 0); border-radius: 4px; color: rgb(255, 255, 255); content: "The video is Muted, Click here to enable sound."; font-size: 12px; font-weight: 600; left: 5px; line-height: 24px; padding: 0px 8px; position: absolute; text-shadow: none; top: -38px; white-space: nowrap; z-index: 2; }
}

